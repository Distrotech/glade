<GladeWidgetClass>
  <Name>GtkCheckButton</Name>
  <GenericName>checkbutton</GenericName>
  <Toplevel>False</Toplevel>

  <Properties>

    <Property Id="border-width"/>
    <Property Id="label"/>
    <Property Id="active">
	  <UpdateSignals>
	    <SignalName>toggled</SignalName>
	  </UpdateSignals>
    </Property>

    <Property Id="draw-indicator" Default="True"/>

  </Properties>
  
</GladeWidgetClass>
	 
	 
	     
