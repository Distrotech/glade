	<property id="orientation">
  	  <displayable-values>
	    <value id="GTK_ORIENTATION_HORIZONTAL" name="Horizontal"/>
	    <value id="GTK_ORIENTATION_VERTICAL" name="Vertical"/>
	  </displayable-values>
	</property>
      	<property id="toolbar-style">
  	  <displayable-values>
	    <value id="GTK_TOOLBAR_ICONS" name="Icons"/>
	    <value id="GTK_TOOLBAR_TEXT" name="Text"/>
	    <value id="GTK_TOOLBAR_BOTH" name="Both"/>
	    <value id="GTK_TOOLBAR_BOTH_HORIZ" name="Both Horizontal"/>
	  </displayable-values>
	</property>
      </properties>
    </glade-widget-class>
    
